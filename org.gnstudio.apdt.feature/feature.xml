All rights reserved. This program and the accompanying materials
are made available under the terms of the GNU Lesser General Public License v3.0
which accompanies this distribution, and is available at
http://www.gnu.org/licenses/gpl-3.0.html

Contributors:
  GNstudio s.r.l. - initial API and implementation
   </license>

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.ui.ide"/>
      <import plugin="org.eclipse.ui.forms"/>
      <import plugin="org.eclipse.emf.ecore" version="2.5.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.emf.ecore.xmi" version="2.5.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.jface.text"/>
      <import plugin="org.eclipse.help"/>
      <import plugin="org.eclipse.ui.views"/>
      <import plugin="org.eclipse.ui.workbench.texteditor"/>
   </requires>

   <plugin
         id="org.gnstudio.apdt"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.gnstudio.apdt.model"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.gnstudio.apdt.model.editor"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
