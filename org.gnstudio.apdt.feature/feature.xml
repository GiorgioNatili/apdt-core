<feature
      id="org.gnstudio.apdt.feature"
      label="Pseudocode Development Tool"
      version="1.1.0.qualifier"
      provider-name="GNstudio s.r.l."
      plugin="org.gnstudio.apdt">

   <description url="http://gnstudio.com/">
      Eclipse plugin for pseudo code an ActionScript generation.
   </description>

